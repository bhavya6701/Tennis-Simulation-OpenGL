# Vertices (v)
# format: 'v' x y z
v -0.5 -0.5 0.5   # Vertex 1
v 0.5 -0.5 0.5    # Vertex 2
v 0.5 0.5 0.5     # Vertex 3
v -0.5 0.5 0.5    # Vertex 4
v -0.5 -0.5 -0.5  # Vertex 5
v 0.5 -0.5 -0.5   # Vertex 6
v 0.5 0.5 -0.5    # Vertex 7
v -0.5 0.5 -0.5   # Vertex 8

# UV Coordinates (vt)
# format: 'vt' u v
vt 0.0 0.0        # UV for Vertex 1
vt 1.0 0.0        # UV for Vertex 2
vt 1.0 1.0        # UV for Vertex 3
vt 0.0 1.0        # UV for Vertex 4
vt 0.0 0.0        # UV for Vertex 5
vt 1.0 0.0        # UV for Vertex 6
vt 1.0 1.0        # UV for Vertex 7
vt 0.0 1.0        # UV for Vertex 8

# Normals (vn)
# format: 'vn' x y z
vn 0.0 0.0 1.0    # Normal for the front face
vn 1.0 0.0 0.0    # Normal for the right face
vn 0.0 0.0 -1.0   # Normal for the back face
vn -1.0 0.0 0.0   # Normal for the left face
vn 0.0 1.0 0.0    # Normal for the top face
vn 0.0 -1.0 0.0   # Normal for the bottom face

# Faces (f) - Vertex Index / UV Index / Normal Index
f 1/1/1 2/2/1 3/3/1     # Front Face
f 1/1/1 3/3/1 4/4/1
f 2/1/2 6/2/2 7/3/2     # Right Face
f 2/1/2 7/3/2 3/4/2
f 6/1/3 5/2/3 8/3/3     # Back Face
f 6/1/3 8/3/3 7/4/3
f 5/1/4 1/2/4 4/3/4     # Left Face
f 5/1/4 4/3/4 8/4/4
f 4/1/5 3/2/5 7/3/5     # Top Face
f 4/1/5 7/3/5 8/4/5
f 5/1/6 6/2/6 2/3/6     # Bottom Face
f 5/1/6 2/3/6 1/4/6
